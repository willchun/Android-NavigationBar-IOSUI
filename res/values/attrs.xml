<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--NavigationBarIos-->
    <declare-styleable name="NavigationBarIos">
        <attr name="NavigationBarIos_mode" format="enum">
            <enum name="NAVIGATION_MODE_STANDARD" value="0"/>
            <enum name="NAVIGATION_MODE_LIST" value="1"/>
            <enum name="NAVIGATION_MODE_TABS" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NavigationBarIosMenu">
        <attr name="NavigationBarIosMenu_Icon_Color" format="color|reference"/>
        <attr name="NavigationBarIosMenu_Icon_Size" format="dimension"/>
        <attr name="NavigationBarIosMenu_Content_Color" format="color|reference"/>
        <attr name="NavigationBarIosMenu_Content_Size" format="dimension"/>
    </declare-styleable>

</resources>